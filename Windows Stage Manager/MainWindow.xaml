<Window x:Class="Windows_Stage_Manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Windows_Stage_Manager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ShowInTaskbar="False">
    <Grid>
        <Label Content="Label" HorizontalAlignment="Left" x:Name="log" VerticalAlignment="Top"/>
        <Button Content="Get Pos" HorizontalAlignment="Left" x:Name="getPosBtn" VerticalAlignment="Top" Click="getPosBtn_Click"/>
    </Grid>
</Window>
